{
  "name": "@tjmora/g-font",
  "version": "1.3.3",
  "description": "A library for including Google Fonts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npx tsc",
    "test1-install": "cd test/client-rendered && npm install",
    "test2-install": "cd test/server-rendered && npm install",
    "test1-build": "cd test/client-rendered && npm run build",
    "test2-build": "cd test/server-rendered && npm run build",
    "test1-devserver": "cd test/client-rendered && npm run start",
    "test2-devserver": "cd test/server-rendered && npm run dev",
    "test1-prodserver": "cd test/client-rendered && serve -s build",
    "test2-prodserver": "cd test/server-rendered && npm run start"
  },
  "keywords": [
    "font",
    "google fonts"
  ],
  "author": "TJ Mora",
  "license": "MIT",
  "homepage": "https://github.com/tjmora/g-font",
  "repository": {
    "type": "git",
    "url": "https://github.com/tjmora/g-font"
  },
  "bugs": {
    "url": "https://github.com/tjmora/g-font/issues"
  },
  "devDependencies": {
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@tjmora/ts-range-types": "^1.1.0"
  }
}
