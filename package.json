{
  "name": "@tjmora/g-font",
  "version": "1.3.4",
  "description": "A library for including Google Fonts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npx tsc",

    "test-angular-install": "cd test/angular && npm install",
    "test-angular-build": "cd test/angular && npm run build --prod",
    "test-angular-devserver": "cd test/angular && npm run start",
    "test-angular-prodserver": "cd test/angular && serve -s dist/angular",

    "test-next-install": "cd test/next && npm install",
    "test-next-build": "cd test/next && npm run build",
    "test-next-devserver": "cd test/next && npm run dev",
    "test-next-prodserver": "cd test/next && npm run start",

    "test-react-install": "cd test/react && npm install",
    "test-react-build": "cd test/react && npm run build",
    "test-react-devserver": "cd test/react && npm run start",
    "test-react-prodserver": "cd test/react && serve -s build"
  },
  "keywords": [
    "font",
    "google fonts"
  ],
  "author": "TJ Mora",
  "license": "MIT",
  "homepage": "https://github.com/tjmora/g-font",
  "repository": {
    "type": "git",
    "url": "https://github.com/tjmora/g-font"
  },
  "bugs": {
    "url": "https://github.com/tjmora/g-font/issues"
  },
  "devDependencies": {
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@tjmora/ts-range-types": "^1.1.0"
  }
}
